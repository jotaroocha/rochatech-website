  <script>
    // Ano no rodapé
    document.getElementById("year").textContent = new Date().getFullYear();

    // Menu mobile (acessível)
    const btn = document.getElementById("navToggle");
    const menu = document.getElementById("primary-menu");

    function setMenu(open) {
      menu.classList.toggle("open", open);
      btn.setAttribute("aria-expanded", String(open));
    }

    if (btn && menu) {
      btn.addEventListener("click", () => setMenu(!menu.classList.contains("open")));
      menu.querySelectorAll("a").forEach((a) => a.addEventListener("click", () => setMenu(false)));

      document.addEventListener("click", (e) => {
        const isClickInside = menu.contains(e.target) || btn.contains(e.target);
        if (!isClickInside) setMenu(false);
      });

      document.addEventListener("keydown", (e) => {
        if (e.key === "Escape") setMenu(false);
      });
    }

    /* ============================
       Instagram: carrossel + embed.js (processa blockquotes)
       ============================ */

    // Troque pelo seu @ (sem @)
    const instagramUsername = "rochatech.net.br";

    // Atualiza links placeholder do perfil (menu/rodapé/botão)
    function setInstagramProfileLink() {
      if (!instagramUsername || instagramUsername === "rochatech.net.br") return;

      const profileUrl = `https://www.instagram.com/${instagramUsername}/`;
      const igProfileBtn = document.getElementById("igProfileBtn");
      if (igProfileBtn) igProfileBtn.href = profileUrl;

      document.querySelectorAll('a[href="https://www.instagram.com/rochatech.net.br/"]').forEach(a => {
        a.href = profileUrl;
      });

      // Atualiza JSON-LD sameAs
      const ld = document.querySelector('script[type="application/ld+json"]');
      if (ld) {
        try {
          const data = JSON.parse(ld.textContent);
          const sameAs = Array.isArray(data.sameAs) ? data.sameAs : [];
          const cleaned = sameAs.filter(x => typeof x === "string" && !x.includes("instagram.com/rochatech.net.br"));
          cleaned.push(profileUrl);
          data.sameAs = cleaned;
          ld.textContent = JSON.stringify(data, null, 2);
        } catch (_) { }
      }
    }

    // Carrossel (scroll-snap)
    const igTrack = document.getElementById("igTrack");
    const igPrev = document.getElementById("igPrev");
    const igNext = document.getElementById("igNext");
    const igSection = document.getElementById("instagram");

    function scrollCarousel(dir) {
      if (!igTrack) return;
      const amount = Math.max(260, Math.round(igTrack.clientWidth * 0.85));
      igTrack.scrollBy({ left: dir * amount, behavior: "smooth" });
    }

    if (igPrev) igPrev.addEventListener("click", () => scrollCarousel(-1));
    if (igNext) igNext.addEventListener("click", () => scrollCarousel(1));

    if (igTrack) {
      igTrack.addEventListener("keydown", (e) => {
        if (e.key === "ArrowLeft") scrollCarousel(-1);
        if (e.key === "ArrowRight") scrollCarousel(1);
      });
    }

    // Lazy-load do script do Instagram (melhor performance)
    function loadInstagramEmbedScript() {
      if (window.__igEmbedLoading) return window.__igEmbedLoading;

      window.__igEmbedLoading = new Promise((resolve, reject) => {
        // Se já existe, apenas processa
        if (window.instgrm && window.instgrm.Embeds && window.instgrm.Embeds.process) {
          window.instgrm.Embeds.process();
          resolve();
          return;
        }

        const s = document.createElement("script");
        s.async = true;
        s.src = "https://www.instagram.com/embed.js";
        s.onload = () => {
          try {
            window.instgrm?.Embeds?.process?.();
          } catch (_) { }
          resolve();
        };
        s.onerror = reject;
        document.body.appendChild(s);
      });

      return window.__igEmbedLoading;
    }

    // Init
    setInstagramProfileLink();

    // Só carrega o embed.js quando a seção estiver próxima (evita pesar o carregamento do site)
    if (igSection) {
      const io = new IntersectionObserver((entries) => {
        if (entries.some(e => e.isIntersecting)) {
          loadInstagramEmbedScript();
          io.disconnect();
        }
      }, { root: null, rootMargin: "300px 0px", threshold: 0.01 });

      io.observe(igSection);
    }

    // ============================
    // Tecnologias: duplica logos para loop contínuo e suave
    // ============================
    function initTechMarquee() {
      const track = document.getElementById("techTrack");
      const group = document.getElementById("techGroup");
      if (!track || !group) return;

      // Evita duplicar duas vezes se você recarregar via hot-reload
      if (track.dataset.cloned === "1") return;

      // Clona 1x para permitir animação -50% (loop contínuo)
      const clone = group.cloneNode(true);
      clone.removeAttribute("id");
      track.appendChild(clone);

      track.dataset.cloned = "1";
    }

    // roda depois que imagens carregarem (mais estável)
    window.addEventListener("load", initTechMarquee);

  </script>
