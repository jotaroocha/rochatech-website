  <style>
    :root {
      --bg: #07070A;
      --text: #F4F4F6;
      --muted: #B6B7BD;
      --line: rgba(244, 244, 246, .10);

      --gold: #DAAA49;
      --goldLine: rgba(218, 170, 73, .28);

      --shadow: 0 12px 30px rgba(0, 0, 0, .45);
      --radius: 14px;

      --max: 1100px;
      --pad: clamp(16px, 3vw, 22px);

      --font-title: "Orbitron", system-ui, -apple-system, Segoe UI, Roboto, Helvetica, Arial, sans-serif;
      --font-body: "Roboto", system-ui, -apple-system, Segoe UI, Arial, sans-serif;

      /* >>> Ajuste fino do "minimalismo" do embed */
      --ig-slide-w: 360px;
      /* largura de cada item no desktop */
      --ig-slide-w-sm: 300px;
      /* largura no mobile */
      --ig-max-h: 420px;
      /* altura máxima (corta o embed pra ficar discreto) */
    }

    * {
      box-sizing: border-box;
    }

    html {
      scroll-behavior: smooth;
    }

    body {
      margin: 0;
      font-family: var(--font-body);
      background:
        radial-gradient(1200px 700px at 50% -10%, rgba(218, 170, 73, .12), transparent 60%),
        radial-gradient(900px 600px at 10% 20%, rgba(218, 170, 73, .08), transparent 55%),
        var(--bg);
      color: var(--text);
      line-height: 1.45;
    }

    a {
      color: inherit;
      text-decoration: none;
    }

    .skip-link {
      position: absolute;
      left: -9999px;
      top: 12px;
      background: rgba(18, 19, 26, .95);
      border: 1px solid var(--goldLine);
      color: var(--text);
      padding: 10px 12px;
      border-radius: 10px;
      z-index: 9999;
    }

    .skip-link:focus {
      left: 12px;
    }

    :focus-visible {
      outline: 2px solid rgba(218, 170, 73, .75);
      outline-offset: 3px;
      border-radius: 10px;
    }

    .wrap {
      width: min(var(--max), calc(100% - (var(--pad) * 2)));
      margin-inline: auto;
    }

    /* Topbar */
    .topbar {
      position: sticky;
      top: 0;
      z-index: 50;
      backdrop-filter: blur(12px);
      background: rgba(7, 7, 10, .62);
      border-bottom: 1px solid rgba(244, 244, 246, .08);
    }

    .nav {
      display: flex;
      align-items: center;
      justify-content: space-between;
      gap: 14px;
      padding: 14px 0;
    }

    .brand {
      display: flex;
      align-items: center;
      gap: 12px;
      min-width: 0;
      font-family: var(--font-title);
      letter-spacing: .4px;
    }

    /* ✅ FIX LOGO: não estica nem comprime */
    .logoImg {
      height: 44px;
      /* mantém altura fixa no header */
      width: auto;
      /* largura automática preserva proporção */
      max-width: 160px;
      /* evita logo gigante em logos horizontais */
      display: block;
      object-fit: contain;
      /* garante que nunca distorce */
      object-position: center;
      border-radius: 12px;
      border: 1px solid var(--goldLine);
      background: rgba(218, 170, 73, .06);
      padding: 4px;
      /* dá respiro caso a logo tenha transparência */
    }

    .brand span {
      display: flex;
      flex-direction: column;
      gap: 2px;
      min-width: 0;
    }

    .brand span small {
      font-size: 12px;
      color: var(--muted);
      font-family: var(--font-body);
      white-space: nowrap;
      overflow: hidden;
      text-overflow: ellipsis;
    }

    .menu {
      display: flex;
      align-items: center;
      gap: 18px;
      font-weight: 700;
      color: rgba(244, 244, 246, .9);
      font-family: var(--font-body);
    }

    .menu a {
      opacity: .92;
      padding: 8px 10px;
      border-radius: 12px;
      transition: background .2s ease, opacity .2s ease;
    }

    .menu a:hover {
      opacity: 1;
      background: rgba(255, 255, 255, .06);
    }

    .btn {
      display: inline-flex;
      align-items: center;
      justify-content: center;
      padding: 11px 14px;
      border-radius: 12px;
      border: 1px solid var(--line);
      background: rgba(255, 255, 255, .04);
      color: var(--text);
      font-weight: 800;
      gap: 10px;
      transition: transform .08s ease, background .2s ease, border-color .2s ease;
      white-space: nowrap;
      letter-spacing: .06px;
      font-family: var(--font-body);
    }

    .btn:hover {
      background: rgba(255, 255, 255, .07);
      border-color: rgba(244, 244, 246, .18);
    }

    .btn:active {
      transform: translateY(1px);
    }

    .btn.primary {
      border-color: rgba(218, 170, 73, .40);
      background: linear-gradient(180deg, rgba(218, 170, 73, .16), rgba(218, 170, 73, .06));
      box-shadow: 0 10px 25px rgba(218, 170, 73, .16);
    }

    .btn.primary:hover {
      border-color: rgba(218, 170, 73, .55);
      background: linear-gradient(180deg, rgba(218, 170, 73, .20), rgba(218, 170, 73, .07));
    }

    /* Mobile menu */
    .navToggle {
      display: none;
      width: 44px;
      height: 44px;
      border-radius: 12px;
      border: 1px solid rgba(244, 244, 246, .14);
      background: rgba(255, 255, 255, .04);
      color: var(--text);
      font-size: 18px;
    }

    @media (max-width: 860px) {
      .menu {
        position: fixed;
        top: 70px;
        right: calc(var(--pad));
        width: min(280px, calc(100% - (var(--pad) * 2)));
        background: rgba(18, 19, 26, .96);
        border: 1px solid rgba(244, 244, 246, .12);
        border-radius: 16px;
        padding: 10px;
        box-shadow: var(--shadow);
        display: none;
        flex-direction: column;
        align-items: stretch;
      }

      .menu.open {
        display: flex;
      }

      .menu a {
        padding: 12px 12px;
      }

      .navToggle {
        display: inline-flex;
        align-items: center;
        justify-content: center;
      }
    }

    /* Sections */
    .section {
      padding: 26px 0;
    }

    .section h2 {
      margin: 0 0 12px;
      font-size: 22px;
      font-family: var(--font-title);
      letter-spacing: .2px;
    }

    .muted {
      color: var(--muted);
    }

    .panel {
      border: 1px solid rgba(244, 244, 246, .10);
      background: rgba(15, 16, 21, .70);
      border-radius: var(--radius);
      box-shadow: var(--shadow);
      overflow: hidden;
      min-width: 0;
    }

    /* Hero */
    .hero {
      padding: 30px 0 22px;
    }

    .heroGrid {
      display: grid;
      grid-template-columns: 1.3fr .7fr;
      gap: 14px;
      align-items: stretch;
    }

    @media (max-width: 900px) {
      .heroGrid {
        grid-template-columns: 1fr;
      }
    }

    .heroMain {
      padding: 22px;
      position: relative;
      isolation: isolate;
    }

    .heroMain::before {
      content: "";
      position: absolute;
      inset: -1px;
      border-radius: var(--radius);
      background:
        radial-gradient(650px 420px at 15% 10%, rgba(218, 170, 73, .22), transparent 60%),
        radial-gradient(520px 380px at 85% 30%, rgba(218, 170, 73, .10), transparent 55%);
      opacity: .9;
      z-index: -1;
    }

    .kicker {
      display: inline-flex;
      align-items: center;
      gap: 10px;
      padding: 8px 10px;
      border: 1px solid rgba(218, 170, 73, .35);
      border-radius: 999px;
      background: rgba(218, 170, 73, .08);
      color: rgba(244, 244, 246, .95);
      font-weight: 900;
      letter-spacing: .3px;
      text-transform: uppercase;
      font-size: 12px;
      font-family: var(--font-body);
    }

    h1 {
      margin: 12px 0 10px;
      font-family: var(--font-title);
      letter-spacing: .2px;
      font-size: 32px;
      line-height: 1.15;
    }

    .sub {
      margin: 0 0 16px;
      color: var(--muted);
      font-size: 15px;
    }

    .ctaRow {
      display: flex;
      gap: 10px;
      flex-wrap: wrap;
      align-items: center;
    }

    .badgeRow {
      display: flex;
      gap: 10px;
      flex-wrap: wrap;
      margin-top: 14px;
    }

    .badge {
      padding: 9px 10px;
      border-radius: 999px;
      border: 1px solid rgba(244, 244, 246, .12);
      background: rgba(255, 255, 255, .04);
      color: rgba(244, 244, 246, .92);
      font-weight: 800;
      font-size: 12px;
      font-family: var(--font-body);
    }

    /* Side panel */
    .heroSide {
      padding: 18px;
      display: flex;
      flex-direction: column;
      gap: 12px;
    }

    .heroSide h2 {
      margin: 0 0 6px;
      font-size: 16px;
      font-family: var(--font-title);
      letter-spacing: .2px;
    }

    .heroSide p {
      margin: 0;
      color: var(--muted);
      font-size: 13px;
    }

    .heroSide .callout {
      border: 1px solid rgba(218, 170, 73, .25);
      background: rgba(218, 170, 73, .06);
      padding: 12px;
      border-radius: 14px;
    }

    .grid3 {
      display: grid;
      gap: 12px;
      grid-template-columns: repeat(auto-fit, minmax(240px, 1fr));
    }

    .steps {
      display: grid;
      gap: 12px;
      grid-template-columns: repeat(auto-fit, minmax(260px, 1fr));
      align-items: stretch;
    }

    .card {
      padding: 16px;
      border: 1px solid rgba(244, 244, 246, .10);
      background: rgba(18, 19, 26, .70);
      border-radius: var(--radius);
    }

    .card h3 {
      margin: 0 0 6px;
      font-size: 16px;
      font-family: var(--font-title);
      letter-spacing: .2px;
    }

    .card p {
      margin: 0;
      color: var(--muted);
      font-size: 14px;
    }

    .planWrap {
      display: grid;
      gap: 12px;
      grid-template-columns: repeat(auto-fit, minmax(260px, 1fr));
      align-items: stretch;
    }

    .plan {
      padding: 18px;
      border: 1px solid rgba(244, 244, 246, .10);
      background: rgba(18, 19, 26, .70);
      border-radius: var(--radius);
      display: flex;
      flex-direction: column;
      gap: 10px;
      position: relative;
      overflow: hidden;
    }

    .plan strong {
      font-family: var(--font-title);
      letter-spacing: .2px;
      font-size: 18px;
    }

    .plan ul {
      margin: 0;
      padding-left: 18px;
      color: var(--muted);
    }

    .plan li {
      margin: 6px 0;
    }

    .pill {
      display: inline-flex;
      width: 34px;
      height: 34px;
      border-radius: 10px;
      align-items: center;
      justify-content: center;
      font-weight: 900;
      background: rgba(218, 170, 73, .10);
      border: 1px solid rgba(218, 170, 73, .32);
      margin-bottom: 8px;
      color: var(--text);
      font-family: var(--font-body);
    }

    /* Footer */
    .footer {
      padding: 22px 0 30px;
      border-top: 1px solid rgba(244, 244, 246, .08);
      background: rgba(10, 10, 14, .55);
    }

    .footerGrid {
      display: grid;
      gap: 14px;
      grid-template-columns: 1.2fr .8fr;
      align-items: start;
    }

    @media (max-width: 900px) {
      .footerGrid {
        grid-template-columns: 1fr;
      }
    }

    .contactBox {
      padding: 14px;
      border-radius: var(--radius);
      border: 1px solid rgba(244, 244, 246, .10);
      background: rgba(18, 19, 26, .70);
    }

    .contactBox a {
      display: block;
      padding: 10px 0;
      border-bottom: 1px dashed rgba(244, 244, 246, .10);
      font-weight: 800;
    }

    .contactBox a:last-child {
      border-bottom: none;
    }

    .contactBox span {
      color: var(--muted);
      font-weight: 600;
    }

    .small {
      font-size: 13px;
      color: var(--muted);
    }

    /* ============================
       Instagram - Carrossel com blockquote (embed oficial)
       ============================ */
    .igHeader {
      display: flex;
      align-items: flex-end;
      justify-content: space-between;
      gap: 12px;
      flex-wrap: wrap;
      margin-bottom: 10px;
    }

    .igCarousel {
      position: relative;
      border: 1px solid rgba(244, 244, 246, .10);
      background: rgba(18, 19, 26, .60);
      border-radius: var(--radius);
      box-shadow: var(--shadow);
      overflow: hidden;
    }

    .igTrack {
      display: flex;
      gap: 12px;
      overflow-x: auto;
      scroll-snap-type: x mandatory;
      padding: 12px;
      -webkit-overflow-scrolling: touch;
      scrollbar-width: thin;
      scrollbar-color: rgba(218, 170, 73, .45) rgba(255, 255, 255, .06);
    }

    .igTrack::-webkit-scrollbar {
      height: 10px;
    }

    .igTrack::-webkit-scrollbar-track {
      background: rgba(255, 255, 255, .06);
      border-radius: 999px;
    }

    .igTrack::-webkit-scrollbar-thumb {
      background: rgba(218, 170, 73, .45);
      border-radius: 999px;
    }

    .igSlide {
      flex: 0 0 var(--ig-slide-w);
      scroll-snap-align: start;
      border-radius: 16px;
      border: 1px solid rgba(244, 244, 246, .10);
      background: rgba(15, 16, 21, .55);
      overflow: hidden;
      position: relative;
      box-shadow: 0 10px 25px rgba(0, 0, 0, .25);
      max-height: var(--ig-max-h);
      /* <<< mantém minimalista */
    }

    @media (max-width: 520px) {
      .igSlide {
        flex-basis: min(var(--ig-slide-w-sm), 85vw);
      }
    }

    /* Fade discreto no fim (indica que está “cortado”) */
    .igSlide::after {
      content: "";
      position: absolute;
      left: 0;
      right: 0;
      bottom: 0;
      height: 64px;
      background: linear-gradient(to bottom, rgba(7, 7, 10, 0), rgba(7, 7, 10, .78));
      pointer-events: none;
    }

    /* Domando o embed do Instagram (mesmo que você cole o blockquote enorme) */
    .igSlide blockquote.instagram-media {
      margin: 0 !important;
      width: 100% !important;
      max-width: 100% !important;
      min-width: 0 !important;

      /* neutraliza o estilo inline “gigante” */
      background: transparent !important;
      border: 0 !important;
      border-radius: 0 !important;
      box-shadow: none !important;
    }

    /* iframe gerado pelo embed.js */
    .igSlide iframe {
      width: 100% !important;
      border: 0 !important;
      border-radius: 0 !important;
      background: transparent !important;
    }

    .igNav {
      position: absolute;
      top: 50%;
      transform: translateY(-50%);
      width: 44px;
      height: 44px;
      border-radius: 14px;
      border: 1px solid rgba(244, 244, 246, .14);
      background: rgba(7, 7, 10, .55);
      color: var(--text);
      display: none;
      align-items: center;
      justify-content: center;
      cursor: pointer;
      z-index: 2;
    }

    .igNav:hover {
      border-color: rgba(218, 170, 73, .40);
    }

    .igNav:active {
      transform: translateY(-50%) translateY(1px);
    }

    @media (min-width: 860px) {
      .igNav {
        display: inline-flex;
      }

      .igNav.prev {
        left: 10px;
      }

      .igNav.next {
        right: 10px;
      }
    }

    .igHint {
      margin-top: 10px;
      font-size: 13px;
      color: var(--muted);
    }

    .igEmpty {
      border: 1px dashed rgba(244, 244, 246, .18);
      background: rgba(18, 19, 26, .45);
      border-radius: var(--radius);
      padding: 14px;
      color: var(--muted);
      margin-top: 10px;
    }

    .igEmpty code {
      color: var(--text);
      background: rgba(255, 255, 255, .06);
      padding: 2px 6px;
      border-radius: 8px;
      border: 1px solid rgba(244, 244, 246, .12);
    }

    /* ============================
   Tecnologias - ticker minimalista (logos rodando)
   ============================ */
    :root {
      --tech-speed: 36s;
      /* menor = mais rápido | maior = mais lento */
    }

    .techHeader {
      display: flex;
      align-items: flex-end;
      justify-content: space-between;
      gap: 12px;
      flex-wrap: wrap;
      margin-bottom: 10px;
    }

    .techMarquee {
      position: relative;
      border: 1px solid rgba(244, 244, 246, .10);
      background: rgba(18, 19, 26, .60);
      border-radius: var(--radius);
      box-shadow: var(--shadow);
      overflow: hidden;
    }

    /* fade nas bordas (fica bem minimalista e elegante) */
    .techMarquee::before,
    .techMarquee::after {
      content: "";
      position: absolute;
      top: 0;
      bottom: 0;
      width: 64px;
      pointer-events: none;
      z-index: 2;
    }

    .techMarquee::before {
      left: 0;
      background: linear-gradient(to right, rgba(7, 7, 10, .92), rgba(7, 7, 10, 0));
    }

    .techMarquee::after {
      right: 0;
      background: linear-gradient(to left, rgba(7, 7, 10, .92), rgba(7, 7, 10, 0));
    }

    .techTrack {
      display: flex;
      width: max-content;
      gap: 0;
      padding: 14px 0;
      will-change: transform;
      animation: techScroll var(--tech-speed) linear infinite;
    }

    /* pausa quando passa o mouse (desktop) */
    .techMarquee:hover .techTrack {
      animation-play-state: paused;
    }

    .techGroup {
      display: flex;
      align-items: center;
      gap: 18px;
      padding: 0 22px;
    }

    .techLogo {
      display: flex;
      align-items: center;
      justify-content: center;
      height: 46px;
      padding: 10px 16px;
      border-radius: 14px;
      border: 1px solid rgba(244, 244, 246, .10);
      background: rgba(15, 16, 21, .50);
      flex: 0 0 auto;
    }

    .techLogo img {
      height: 24px;
      /* mantém tudo alinhado */
      width: auto;
      /* preserva proporção (não distorce) */
      max-width: 150px;
      object-fit: contain;
      opacity: .92;
      filter: saturate(1.02) contrast(1.02);
    }

    .techNote {
      margin: 10px 0 0;
      font-size: 12px;
      color: var(--muted);
    }

    @keyframes techScroll {
      from {
        transform: translateX(0);
      }

      to {
        transform: translateX(-50%);
      }

      /* funciona porque duplicamos o conteúdo via JS */
    }

    /* acessibilidade: respeita "reduzir movimento" */
    @media (prefers-reduced-motion: reduce) {
      .techTrack {
        animation: none;
      }
    }
  </style>
</head>

<body>
  <a class="skip-link" href="#conteudo">Pular para o conteúdo</a>

  <!-- Topo -->
  <header class="topbar" role="banner">
    <div class="wrap">
      <div class="nav">
        <a class="brand" href="#topo" aria-label="Ir para o topo">
          <img class="logoImg" src="https://www.rochatech.net.br/assets/images/logo.png" alt="Logo RochaTech"
            loading="eager" fetchpriority="high" />
          <span>
            RochaTech
